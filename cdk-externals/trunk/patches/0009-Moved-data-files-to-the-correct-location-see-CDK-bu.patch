+                                        <mi>v</mi>
+                                        <mi>i</mi>
+                                    </msub>
+                                </mrow>
+                                <mrow>
+                                    <msub>
+                                        <mi>v</mi>
+                                        <mi>j</mi>
+                                    </msub>
+                                </mrow>
+                                <mo>)</mo>
+                            </mrow>
+                            <mi>0.5</mi>
+                        </msup>
