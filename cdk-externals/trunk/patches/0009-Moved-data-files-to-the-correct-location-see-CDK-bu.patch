+            <mrow xmlns="http://www.w3.org/1998/Math/MathML">
+                <mi>X</mi>
+                <mo>=</mo>
+                <mi>a</mi>
+                <mo>+</mo>
+                <mi>b</mi>
+                <mi>q</mi>
+                <mo>+</mo>
+                <mi>c</mi>
+                <mrow>
+                    <mo>(</mo>
+                    <mrow>
