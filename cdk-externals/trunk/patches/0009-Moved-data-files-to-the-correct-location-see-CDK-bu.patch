+    <Descriptor rdf:ID="lipinskifailures">
+        <rdfs:label>Lipinski's Rule of Five</rdfs:label>
+        <annotation>
+            <documentation>
+                <dc:contributor rdf:resource="#mf"/>
+                <dc:contributor rdf:resource="#elw"/>
